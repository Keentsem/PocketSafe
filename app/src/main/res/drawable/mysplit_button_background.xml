<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#E6C34E" />
            <stroke android:width="2dp" android:color="#5b3f2c" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="oval">
            <solid android:color="#f3c34e" />
            <stroke android:width="2dp" android:color="#5b3f2c" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#80f3c34e" />
            <stroke android:width="1dp" android:color="#805b3f2c" />
        </shape>
    </item>
</selector>
